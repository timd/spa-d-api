{
  "name": "divorcy",
  "version": "1.16.2",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "format": "prettier-eslint \"$PWD/src/**/*.{js,jsx}\" --write",
    "lint:js": "eslint 'src/**/*.{js,jsx}'",
    "lint:css": "stylelint 'src/**/*.{css,jsx}'",
    "lint": "yarn lint:js & yarn lint:css",
    "eject": "react-scripts eject"
  },
  "config-overrides-path": "src/config/config-overrides",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mocha": "^7.1.2",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "raw-loader": "^4.0.1",
    "react-app-rewired": "^2.1.5",
    "react-docgen-external-proptypes-handler": "^2.0.0",
    "react-styleguidist": "^11.0.3",
    "string-replace-loader": "^2.3.0",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "dependencies": {
    "@ivoryio/kogaio": "^0.7.91",
    "@reach/router": "^1.3.3",
    "formik": "^2.1.4",
    "i18next": "^19.4.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.3.5",
    "react-markdown": "^4.3.1",
    "react-scripts": "^3.4.0",
    "styled-components": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint:js --fix"
    ],
    "*.{jsx,css}": [
      "yarn lint:css"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  }
}